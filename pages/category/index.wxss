.cate_container {
  /* 高度 等于视口的高度 - 头部搜索框的高度 
     calc 100vh 不要less来编译 calc中的代码
  */
  height: calc(100vh + 90rpx);
  display: flex;
}
.cate_container .left_menu {
  flex: 1;
}
.cate_container .left_menu .menu_title {
  height: 80rpx;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
  display: flex;
}
.cate_container .left_menu .active {
  color: var(--themeColor);
  border-left: 4rpx solid currentColor;
}
.cate_container .right_menu {
  flex: 3;
}
.cate_container .right_menu .good_group .good_title {
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.cate_container .right_menu .good_group .good_title .delimiter {
  color: #ccc;
}
.cate_container .right_menu .good_group .good_title .good_name {
  font-size: 30rpx;
  color: #000;
  padding: 0 10rpx;
}
.cate_container .right_menu .good_group .good_list {
  display: flex;
  flex-wrap: wrap;
}
.cate_container .right_menu .good_group .good_list navigator {
  width: 33.33%;
  text-align: center;
}
.cate_container .right_menu .good_group .good_list navigator image {
  width: 50%;
}
